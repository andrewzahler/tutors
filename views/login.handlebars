<main>
    <div class="container">
        <div class="row">
            <div class="col s5 offset-s1 left">
                <div class="row formDiv">
                    <h4>Log In to Your Account</h4>
                    <form action="login" id="login" method="post" class="col s12" name="login">
                    <div class="input-field s12">
                        <i class="material-icons prefix">account_circle</i>
                        <input id="email" type="text" class="validate" name="email">
                        <label for="email">Email</label>
                    </div>
                    <div class="input-field s12">
                        <i class="material-icons prefix">lock</i>
                        <input id="password" type="password" class="validate" name="password">
                        <label for="password">Password</label>
                    </div>
                    <div class="col s6 offset-s3">
                        <button class="btn waves-effect waves-light" type="submit" value="Sign In">Submit
                            <i class="material-icons right">send</i>
                        </button>
                    </div>
                    </form>
                    {{!-- <div class="col s12 textDiv">
                    <p>Don't have an account? <a href="/register">Sign up here</a>.</p>
                    </div> --}}
                </div>
            </div>
            <div class="col s5">
                <div class="row formDiv">
                    <h4>Or Register for One Now</h4>
                    <form action="/register" method="post" class="col s12" name="register">
                        <div class="input-field col s12">
                        <i class="material-icons prefix">account_circle</i>
                          <input id="email" type="email" name="email" class="validate">
                          <label for="email" data-error="wrong" data-success="right">Email</label>
                        </div>
                      
                        <div class="input-field col s12">
                        <i class="material-icons prefix">lock</i>
                          <input id="password" type="password" name="password" class="validate">
                          <label for="password">Password</label>
                        </div>
                      
                      <div class="col s6 offset-s3">
                        <button class="btn waves-effect waves-light" type="submit" value="Sign In">Submit
                            <i class="material-icons right">send</i>
                        </button>
                    </div>
                  </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
