    <main>
    <div class="container">
      <div class="row">
        <div class="col s12">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Welcome {{students.name}}</span>
              <!-- <p>I am a very simple card. I am good at containing small bits of information.
              I am convenient because I require little markup to use effectively.</p> -->
            </div>
            <div style="overflow: auto" class="card-action">
              <img style="float: left; margin-right: 30px;" src="http://lorempixel.com/200/200/cats" />
              <p>Name:{{students.name}} </p>
              <p>Phone: {{students.phone}}</p>
              <p>Address:{{students.address}}</p>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col s12 header-bg">
          <h4 class="header-txt">My Appointments</h4>
          <div class="appointments">
            <table>
              <thead>
                <tr>
                    <th>Date</th>
                    <th>Time</th>
                    <th>Duration</th>
                    <th>Subject</th>
                    <th>Tutor</th>
                </tr>
              </thead>

              <tbody>
                <tr>
                 {{#each students.Appointments}}
                    <td>{{this.date}}</td>
                    <td>{{this.time}}</td>
                     <td>{{this.hours}}</td>
                     <td>{{this.subject}}</td>
                  {{/each}}
                </tr>
              </tbody>
            </table>
          </div>
        </div>
        
      </div>
    </div>

    </main>
